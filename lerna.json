{
  "packages": [
    "packages/*",
    "frontend/*",
    "backend/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "command": {
    "publish": {
      "npmClient": "npm",
      "allowBranch": [
        "master"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  }
}
