{
  "name": "noak",
  "version": "1.0.0",
  "author": "Jonathan Corral <contact@jonathan-corral.fr>",
  "license": "MIT",
  "keywords": [
    "react",
    "noak",
    "next.js"
  ],
  "repository": "git+https://github.com/j-corral/noak.git",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "clean": "lerna clean -y && lerna bootstrap && rimraf ./dist",
    "build": "yarn prepublish",
    "build:lib": "yarn workspace packages run build",
    "dev": "lerna run dev --parallel --stream",
    "prepublish": "lerna run prepublish",
    "test": "lerna run test --scope={@noak/components}",
    "test-types": "yarn tsc",
    "diff": "lerna diff",
    "new-version": "lerna version --conventional-commits --yes"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "eslint": "^8.2.0",
    "lerna": "^4.0.0",
    "next": "^12.0.3",
    "next-translate": "^1.1.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
